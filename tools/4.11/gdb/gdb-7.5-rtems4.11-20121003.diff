@@ -103,6 +103,7 @@
 {
   /* Take yet unprocessed bytes into account.  */
   md5_uint32 bytes = ctx->buflen;
+  md5_uint32 swap_bytes;
   size_t pad;
 
   /* Now count remaining bytes.  */
@@ -113,10 +114,13 @@
   pad = bytes >= 56 ? 64 + 56 - bytes : 56 - bytes;
